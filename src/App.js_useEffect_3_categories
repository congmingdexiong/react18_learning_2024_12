import React, { useEffect, useState } from 'react';

export default function App() {
  const [count, setCount] = useState(0);

  useEffect(() => {
    console.log('副作用产生了');
  }, [count]);
  return (
    <div>
      App
      <button onClick={() => setCount(count + 1)}>+{count}</button>
    </div>
  );
}
